class Father
{
    void surName()
    {
        System.out.println("k");
    }
    void designation()
{
    System.out.println("Clerk");
    }
}
class Child extends Father
{
    void getName()
    {
        System.out.print("Sathish");
    }
    void designation()
    {
        System.out.println("Java Programmer");
    }
}
public class Main
{
    public static void main(String[] args)
    {
        Child c1=new Child();
        c1.getName();
        c1.surName();
        c1.designation();
    }
}
    
